{
  "name": "api3-tracker",
  "version": "0.1.0",
  "node": ">=16.13.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:init": "docker run --name pgapi3dao -e POSTGRES_DB=api3dao -e POSTGRES_PASSWORD=test -d -p5432:5432 postgres",
    "db:shell": "PGPASSWORD=test psql -U postgres -h 127.0.0.1",
    "db:rm": "docker rm -f pgapi3dao",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,css,scss,md,json,prisma}\" --loglevel silent",
    "format:check": "prettier --check \"./**/*.{ts,tsx,js,jsx,css,scss,md,json,prisma}\" --loglevel debug",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.15.2",
    "axios": "^0.27.2",
    "next": "12.1.6",
    "next-auth": "^4.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^3.14.0",
    "prisma": "^4.0.0",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
